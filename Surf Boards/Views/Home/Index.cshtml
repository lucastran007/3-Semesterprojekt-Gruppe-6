@model List<Surf_Boards.Models.SurfBoard>
@{

}
<h1>Surfboards </h1>

<table class="table">
    <thead>
        <tr>
            <th>BoardName </th>
            <th>Length </th>
            <th>Width</th>
            <th>Thickness</th>
            <th>Volume</th>
            <th>Boardtype</th>
            <th>Price</th>
            <th>Equipment</th>
            <th>Billede</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var surfBoard in Model)
        {
            <tr>
                <td>@surfBoard.BoardName</td>
                <td>@surfBoard.Length</td>
                <td>@surfBoard.Width</td>
                <td>@surfBoard.Thickness</td>
                <td>@surfBoard.Volume</td>
                <td>@surfBoard.Boardtype</td>
                <td>@surfBoard.Price</td>
                <td>@surfBoard.Equipment</td>
                <td><img src="@("~/Images/"+surfBoard.ImageName)" asp-append-version="true" width="25px" height="25px" /></td>
            </tr>
        }
    </tbody>
</table>
